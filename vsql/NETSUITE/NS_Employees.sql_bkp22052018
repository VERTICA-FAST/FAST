/****
****Script Name	  : NS_Employees.sql
****Description   : Incremental data load for NS_Employees
****/

/* Setting timing on**/
\timing
/**SET SESSION AUTOCOMMIT TO OFF;**/

\set ON_ERROR_STOP on

CREATE LOCAL TEMP TABLE Start_Time_Tmp ON COMMIT PRESERVE ROWS AS select count(*) count, sysdate st from "swt_rpt_stg"."NS_Employees";

/* Inserting values into Audit table  */

INSERT INTO swt_rpt_stg.FAST_LD_AUDT
(
SUBJECT_AREA
,TBL_NM
,LD_DT
,START_DT_TIME
,END_DT_TIME
,SRC_REC_CNT
,TGT_REC_CNT
,COMPLTN_STAT
)
select 'NETSUITE','NS_Employees',sysdate::date,sysdate,null,(select count from Start_Time_Tmp) ,null,'N';

Commit;


CREATE LOCAL TEMP TABLE NS_Employees_stg_Tmp ON COMMIT PRESERVE ROWS AS
(
SELECT DISTINCT * FROM swt_rpt_stg.NS_Employees)
SEGMENTED BY HASH(employee_id,last_modified_date) ALL NODES;

INSERT /*+DIRECT*/ INTO "swt_rpt_stg".NS_Employees_Hist SELECT * from "swt_rpt_stg".NS_Employees;
COMMIT;
TRUNCATE TABLE swt_rpt_stg.NS_Employees;

CREATE LOCAL TEMP TABLE NS_Employees_base_Tmp ON COMMIT PRESERVE ROWS AS
(
SELECT DISTINCT employee_id,last_modified_date FROM swt_rpt_base.NS_Employees)
SEGMENTED BY HASH(employee_id,last_modified_date) ALL NODES;


CREATE LOCAL TEMP TABLE NS_Employees_stg_Tmp_Key ON COMMIT PRESERVE ROWS AS
(
SELECT employee_id, max(last_modified_date) as last_modified_date FROM NS_Employees_stg_Tmp group by employee_id)
SEGMENTED BY HASH(employee_id,last_modified_date) ALL NODES;

/* Inserting Stage table data into Historical Table */

