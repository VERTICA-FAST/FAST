FROM "swt_rpt_base".NS_Employees WHERE employee_id in 
(SELECT STG.employee_id FROM NS_Employees_stg_Tmp_Key STG JOIN NS_Employees_base_Tmp
ON STG.employee_id = NS_Employees_base_Tmp.employee_id AND STG.last_modified_date >= NS_Employees_base_Tmp.last_modified_date);




/* Deleting before seven days data from current date in the Historical Table */

delete /*+DIRECT*/ from "swt_rpt_stg"."NS_Employees_Hist"  where LD_DT::date <= TIMESTAMPADD(DAY,-7,sysdate)::date;



/* Incremental VSQL script for loading data from Stage to Base */


DELETE /*+DIRECT*/ FROM "swt_rpt_base".NS_Employees WHERE employee_id in 
(SELECT STG.employee_id FROM NS_Employees_stg_Tmp_Key STG JOIN NS_Employees_base_Tmp
ON STG.employee_id = NS_Employees_base_Tmp.employee_id AND STG.last_modified_date >= NS_Employees_base_Tmp.last_modified_date);

