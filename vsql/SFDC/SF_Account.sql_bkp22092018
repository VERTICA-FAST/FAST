/****
****Script Name   : SF_Account.sql
****Description   : Incremental data load for SF_Account
****/

/**SET SESSION AUTOCOMMIT TO OFF;**/

--SET SESSION AUTOCOMMIT TO OFF;

/*Setting timing on */
\timing

\set ON_ERROR_STOP on

CREATE LOCAL TEMP TABLE Start_Time_Tmp ON COMMIT PRESERVE ROWS AS select count(*) count, sysdate st from "swt_rpt_stg"."SF_Account";

/* Inserting values into Audit table  */

INSERT INTO swt_rpt_stg.FAST_LD_AUDT
(
SUBJECT_AREA
,TBL_NM
,LD_DT
,START_DT_TIME
,END_DT_TIME
,SRC_REC_CNT
,TGT_REC_CNT
,COMPLTN_STAT
)
select 'SFDC','SF_Account',sysdate::date,sysdate,null,(select count from Start_Time_Tmp) ,null,'N';

Commit;

INSERT /*+Direct*/ INTO swt_rpt_stg.SF_Account_Hist SELECT * from swt_rpt_stg.SF_Account;
COMMIT;

CREATE LOCAL TEMP TABLE duplicates_records ON COMMIT PRESERVE ROWS AS (
select id,max(auto_id) as auto_id from swt_rpt_stg.SF_Account where id in (
select id from swt_rpt_stg.SF_Account group by id,LASTMODIFIEDDATE having count(1)>1)
group by id);


delete from swt_rpt_stg.SF_Account where exists(
select 1 from duplicates_records t2 where swt_rpt_stg.SF_Account.id=t2.id and swt_rpt_stg.SF_Account.auto_id<t2.auto_id);

commit;

CREATE LOCAL TEMP TABLE SF_Account_stg_Tmp ON COMMIT PRESERVE ROWS AS
(
SELECT DISTINCT * FROM swt_rpt_stg.SF_Account)
SEGMENTED BY HASH(ID,LASTMODIFIEDDATE) ALL NODES;

TRUNCATE TABLE swt_rpt_stg.SF_Account;

CREATE LOCAL TEMP TABLE SF_Account_base_Tmp ON COMMIT PRESERVE ROWS AS
(
SELECT DISTINCT ID,LASTMODIFIEDDATE FROM swt_rpt_base.SF_Account)
SEGMENTED BY HASH(ID,LASTMODIFIEDDATE) ALL NODES;


CREATE LOCAL TEMP TABLE SF_Account_stg_Tmp_Key ON COMMIT PRESERVE ROWS AS
(
SELECT id, max(lastmodifieddate) as lastmodifieddate FROM SF_Account_stg_Tmp group by id)
SEGMENTED BY HASH(ID,LASTMODIFIEDDATE) ALL NODES;


