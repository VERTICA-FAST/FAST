/* Deleting before seven days data from current date in the Historical Table */

/*delete /*+DIRECT*/ from "swt_rpt_stg"."SF_Account_HIST"  where LD_DT::date <= TIMESTAMPADD(DAY,-7,sysdate)::date; */


/* Incremental VSQL script for loading data from Stage to Base */

DELETE /*+DIRECT*/ FROM "swt_rpt_base".SF_Account WHERE id in
(SELECT STG.id FROM SF_Account_stg_Tmp_Key STG JOIN SF_Account_base_Tmp
ON STG.id = SF_Account_base_Tmp.id AND STG.LastModifiedDate >= SF_Account_base_Tmp.LastModifiedDate);

